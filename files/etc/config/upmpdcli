config upmpdcli wifi
	option interface 'wlan0'
	option friendly_name 'AirSpeaker'
	option mpd_host '127.0.0.1'
	option mpd_port '6600'
	option upmpd_port '0'
	option config '/etc/upmpdcli.conf'

